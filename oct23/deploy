pipeline {
    agent any
    tools {
      maven 'mymaven'
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
stage('Deploy') {
            steps {
                sshagent(['1d24a241-6c3e-44f8-8e8a-01f1ca435584']) {
    sh "scp -o StrictHostKeyChecking=no target/*.war gbemi@54.226.153.78:/home/admin1/tomcat/webapps"
}
            }
        }
    }
}
