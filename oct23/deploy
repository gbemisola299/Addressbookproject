pipeline {
    agent any
    tools {
      maven 'mymaven'
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
stage('Deploy') {
            steps {
                sshagent(['98498650-58ad-412f-9ead-78dab64903a8']) {
    sh "scp -o StrictHostKeyChecking=no target/*.war gbemi@54.82.160.93:/home/admin1/tomcat/webapps"
}
            }
        }
    }
}
